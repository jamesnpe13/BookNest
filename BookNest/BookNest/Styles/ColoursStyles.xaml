<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Accents  -->
    <!--<SolidColorBrush x:Key="Primary" Color="#FF4B923A" />-->
    <!--<SolidColorBrush x:Key="Primary" Color="#FF33BC69" />-->
    <SolidColorBrush x:Key="Primary" Color="#FF54A142" />
    <SolidColorBrush x:Key="Secondary" Color="#FF4F9FFF" />
    <SolidColorBrush x:Key="LightBlue" Color="#FFCECECE" />
    <SolidColorBrush x:Key="Destructive" Color="#FFAB3E3E" />

    <SolidColorBrush x:Key="Error" Color="#FFAB3E3E" />
    <SolidColorBrush x:Key="Success" Color="#FF54A142" />
    <SolidColorBrush x:Key="Default" Color="#FF4491E0" />
    <SolidColorBrush x:Key="Warning" Color="#FFFFBB0B" />

    <!--  Blue tones  -->
    <SolidColorBrush x:Key="Blue1" Color="#3C4661" />
    <SolidColorBrush x:Key="Blue2" Color="#2B3349" />
    <SolidColorBrush x:Key="Blue3" Color="#FF2D3345" />
    <SolidColorBrush x:Key="Blue4" Color="#FF252C3B" />
    <SolidColorBrush x:Key="Blue5" Color="#22242C" />

    <!--  Black and white  -->
    <SolidColorBrush x:Key="Black" Color="#FF16171A" />
    <SolidColorBrush x:Key="White" Color="#F2F2F2" />

    <!--  Grey tones  -->
    <SolidColorBrush x:Key="Grey1" Color="#FFA2A3A7" />
    <SolidColorBrush x:Key="Grey2" Color="#FF72747F" />
    <SolidColorBrush x:Key="Grey3" Color="#FF494B56" />
    <SolidColorBrush x:Key="Grey4" Color="#2F323F" />

</ResourceDictionary>